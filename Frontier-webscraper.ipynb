   "source": [
    "# Create the OpenAI client instance for OpenRouter\n",
    "openai = OpenAI(\n",
    "    api_key=os.getenv('DEEPSEEK_R1_API_KEY'),  # Your OpenRouter API key\n",
    "    base_url=\"https://openrouter.ai/api/v1\"\n",
    ")"
   ]
